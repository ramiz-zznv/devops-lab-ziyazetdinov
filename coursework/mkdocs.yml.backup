site_name: Рамиз Зиязетдинов
site_description: Сайт-портфолио 
site_author: Рамиз Зиязетдинов
site_url: https://ramiz-zznv.github.io/devops-lab-ziyazetdinov/coursework/

theme:
  name: material
  features:
    - navigation.tabs
    - navigation.sections
    - search.highlight
    - search.suggest
  palette:
    - scheme: default
      primary: blue
      accent: light blue
    - scheme: slate
      primary: blue
      accent: blue

nav:
  - Главная: index.md
  - О себе: about.md
  - Проекты: projects.md
  - Контакты: contacts.md

markdown_extensions:
  - pymdownx.superfences
  - admonition
  - pymdownx.details
  - tables

extra:
  social:
    - icon: fontawesome/brands/telegram
      link: https://t.me/ramiz_zzv
      name: Telegram
    - icon: fontawesome/solid/envelope
      link: mailto:ramiz.zznv@gmail.com
      name: Email
